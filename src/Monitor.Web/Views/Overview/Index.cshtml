@model dynamic

@{
    ViewBag.Title = "Overview";
}

<div class="machines">
    <ul data-bind="foreach: machineStateViewModels">
    	<li>
    		<span data-bind="text: MachineName"></span>
    		<ul data-bind="foreach: DataSeries">
    			<li>
    				<span data-bind="text: Name"></span>
    				<span data-bind="text: LastPoint"></span>
    			</li>
    		</ul>
    	</li>
    </ul>
</div>

@section CustomScripts {
    <script src="http://code.highcharts.com/highcharts.js"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/systemmonitor.base.js")"></script>
}

